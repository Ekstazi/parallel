{
    "name": "amphp/parallel",
    "description": "Parallel processing component for Amp.",
    "keywords": [
        "asynchronous",
        "async",
        "concurrent",
        "multi-threading",
        "multi-processing"
    ],
    "homepage": "https://github.com/amphp/parallel",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Aaron Piotrowski",
            "email": "aaron@trowski.com"
        },
        {
            "name": "Stephen Coakley",
            "email": "me@stephencoakley.com"
        },
        {
            "name": "Niklas Keller",
            "email": "me@kelunik.com"
        }
    ],
    "require": {
        "amphp/amp": "dev-ext-async",
        "amphp/byte-stream": "dev-ext-async",
        "amphp/parser": "^1",
        "amphp/process": "dev-ext-async",
        "amphp/sync": "dev-ext-async"
    },
    "require-dev": {
        "phpunit/phpunit": "^6",
        "amphp/phpunit-util": "dev-ext-async",
        "friendsofphp/php-cs-fixer": "^2.3"
    },
    "suggest": {
        "ext-pthreads": "Required for thread contexts"
    },
    "autoload": {
        "psr-4": {
            "Amp\\Parallel\\": "lib"
        },
        "files": [
            "lib/Worker/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Amp\\Parallel\\Example\\": "examples",
            "Amp\\Parallel\\Test\\": "test"
        }
    },
    "config": {
        "platform": {
            "php": "7.2.0"
        }
    }
}
